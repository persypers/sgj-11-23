  m_Script: {fileID: 11500000, guid: 0c1c47cbf3af15f4fa5be4f181cfd171, type: 3}
  m_Name: AttendantSpeech
  m_EditorClassIdentifier: 
  states:
  - StateName: Default
    lines:
    - text: "\u041A\u0430\u043A \u0434\u0443\u043C\u0430\u0435\u0448\u044C, \u044F
        \u0431\u044B \u0441\u043C\u043E\u0433\u043B\u0430 \u0431\u044B\u0442\u044C
        \u043C\u0430\u0448\u0438\u043D\u0438\u0441\u0442\u043E\u043C?"
    - text: "\u0420\u0435\u043B\u044C\u0441\u044B-\u0440\u0435\u043B\u044C\u0441\u044B,
        \u0448\u043F\u0430\u043B\u044B-\u0448\u043F\u0430\u043B\u044B, \u0441\u043A\u043E\u0440\u0435\u0439
        \u0431\u044B \u0432\u044B\u0445\u043E\u0434\u043D\u043E\u0439."
    - text: "\u041A\u0430\u043A \u0442\u0430\u043C \u0413\u0435\u043D\u043A\u0430,
        \u043D\u0435 \u0443\u0441\u043D\u0443\u043B \u0435\u0449\u0451?"
    - text: "\u0411\u0435\u043B\u044C\u0435 \u0440\u0430\u0437\u043D\u0435\u0441\u0438,
        \u0447\u0430\u0439 \u043F\u043E\u0434\u0430\u0439, \u0430 \u0440\u0430\u0441\u0442\u0438
        \u0434\u0443\u0445\u043E\u0432\u043D\u043E \u043A\u043E\u0433\u0434\u0430?"
    - text: "\u042F \u0431\u044B \u043C\u043E\u0436\u0435\u0442 \u0432\u044B\u0443\u0447\u0438\u0442\u044C\u0441\u044F
        \u0445\u043E\u0442\u0435\u043B\u0430, \u043D\u0430 \u0431\u0443\u0445\u0433\u0430\u043B\u0442\u0435\u0440\u0430.
        \u0414\u0430 \u0441 \u0447\u0435\u0433\u043E \u043D\u0430\u0447\u0430\u0442\u044C?"
    - text: "\u0412\u043E\u0442 \u0431\u044B \u043F\u0440\u043E\u0447\u0435\u0441\u0442\u044C
        \u0442\u0430\u043A\u0443\u044E \u043A\u043D\u0438\u0433\u0443, \u0447\u0442\u043E\u0431\u044B
        \u0432\u0441\u044F \u0436\u0438\u0437\u043D\u044C \u0438\u0437\u043C\u0435\u043D\u0438\u043B\u0430\u0441\u044C!"
  - StateName: Newspaper
    lines:
    - text: "\u0412\u043E\u0442 \u0436\u0435... \u0412 \u0433\u043E\u0440\u043E\u0441\u043A\u043E\u043F\u0435
        \u043F\u0438\u0448\u0443\u0442 \"\u043E\u0442\u043A\u0440\u043E\u0439\u0442\u0435\u0441\u044C
        \u043D\u043E\u0432\u043E\u043C\u0443\". \u0410 \u0447\u0442\u043E \u0442\u0443\u0442
        \u043C\u043E\u0436\u0435\u0442 \u0431\u044B\u0442\u044C \u043D\u043E\u0432\u043E\u0433\u043E?"
  - StateName: TrainSchedule
    lines:
    - text: "\u0420\u0430\u0441\u043F\u0438\u0441\u0430\u043D\u0438\u0435? \u041D\u0430
        \u0442\u0430\u043A\u043E\u0435 \u043F\u0430\u0441\u0441\u0430\u0436\u0438\u0440\u044B
        \u0441\u043E \u0441\u043A\u0443\u043A\u0438 \u0441\u043C\u043E\u0442\u0440\u044F\u0442,
        \u043E\u0441\u043E\u0431\u0435\u043D\u043D\u043E \u0434\u0435\u0442\u043A\u0438."
  - StateName: Easel
    lines:
    - text: "\u041A\u0430\u043A\u043E\u0439 \u0438\u0437\u044F\u0449\u043D\u044B\u0439,
        \u0442\u043E\u043D\u043A\u0438\u0439... \u0441\u043B\u043E\u0432\u043D\u043E
        \u0441\u043E\u0437\u0434\u0430\u043D \u0434\u043B\u044F \u0434\u0435\u0432\u0443\u0448\u043A\u0438"
  - StateName: Laptop
    lines:
    - text: "\u0412\u043E\u0442 \u0442\u0430\u0441\u043A\u0430\u044E\u0442 \u0430\u0439\u0442\u0438\u0448\u043D\u0438\u043A\u0438
        \u0441\u0432\u043E\u0438 \u043A\u043E\u043C\u043F\u044C\u044E\u0442\u0435\u0440\u044B,
        \u0430 \u044F \u0438\u043C \u0440\u043E\u0437\u0435\u0442\u043A\u0438 \u043F\u043E\u043A\u0430\u0437\u044B\u0432\u0430\u0439"
  - StateName: Eggs
    lines:
    - text: "\u0420\u0430\u043D\u044C\u0448\u0435 \u0433\u043E\u0432\u043E\u0440\u044F\u0442
        \u0442\u043E\u043B\u044C\u043A\u043E \u0442\u0430\u043A \u0438 \u0435\u0437\u0434\u0438\u043B\u0438,
        \u0441 \u044F\u0438\u0447\u043A\u0430\u043C\u0438, \u0441 \u043A\u0443\u0440\u043E\u0447\u043A\u043E\u0439.
        \u0421\u0442\u0430\u0440\u0438\u043A\u0438 \u0434\u043E \u0441\u0438\u0445
        \u043F\u043E\u0440 \u0441 \u0441\u043E\u0431\u043E\u0439 \u0431\u0435\u0440\u0443\u0442"
  - StateName: Book
    lines:
    - text: "\u041C\u043E\u044F \u043B\u044E\u0431\u0438\u043C\u0430\u044F \u0432
        \u0441\u0435\u0440\u0438\u0438!"
