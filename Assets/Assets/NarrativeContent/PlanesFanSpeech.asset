  m_Script: {fileID: 11500000, guid: 0c1c47cbf3af15f4fa5be4f181cfd171, type: 3}
  m_Name: PlanesFanSpeech
  m_EditorClassIdentifier: 
  states:
  - StateName: Default
    lines:
    - text: "\u0412\u043E\u0442 \u0431\u044B \u043F\u043E\u043B\u0435\u0442\u0430\u0442\u044C\u2026"
    - text: "\u0414\u0430-\u0430-\u0430, \u043D\u0430\u0434\u043E \u0431\u044B\u043B\u043E
        \u043D\u0430 \u0441\u0430\u043C\u043E\u043B\u0451\u0442\u0435 \u043B\u0435\u0442\u0435\u0442\u044C."
    - text: "\u041A\u043E\u0433\u0434\u0430 \u0441\u0430\u043C\u043E\u043B\u0451\u0442\u044B
        \u043E\u0442\u043A\u0430\u0436\u0443\u0442\u0441\u044F \u043E\u0442 \u043A\u043E\u043B\u0451\u0441,
        \u043E\u043D\u0438 \u043D\u0430\u043A\u043E\u043D\u0435\u0446-\u0442\u043E
        \u0441\u0442\u0430\u043D\u0443\u0442 \u0441\u043E\u0432\u0435\u0440\u0448\u0435\u043D\u043D\u044B."
    - text: "\u0421\u0435\u0439\u0447\u0430\u0441 \u0431\u044B \u043F\u0438\u043B
        \u0441\u0430\u043C\u044B\u0439 \u0432\u043A\u0443\u0441\u043D\u044B\u0439
        \u0442\u043E\u043C\u0430\u0442\u043D\u044B\u0439 \u0441\u043E\u043A."
    - text: "\u041F\u043E\u0435\u0437\u0434\u2026 \u042D\u0442\u043E \u0442\u0430\u043A
        \u0441\u0442\u0430\u0440\u043E\u043C\u043E\u0434\u043D\u043E."
  - StateName: Eggs
    lines:
    - text: "\u042D\u0442\u043E \u0447\u0442\u043E, \u043E\u0431\u0435\u0434 \u0441
        \u0441\u043E\u0431\u043E\u0439 \u043A\u0442\u043E-\u0442\u043E \u0432\u0435\u0437\u0451\u0442?
        \u041D\u0443 \u0438 \u043D\u0443, \u0441\u0440\u0435\u0434\u043D\u0435\u0432\u0435\u043A\u043E\u0432\u044C\u0435!"
  - StateName: VinylPlayer
    lines:
    - text: "\u0412\u043E\u0442 \u044D\u0442\u043E \u0432\u0438\u043D\u0442\u0430\u0436\u043D\u0430\u044F
        \u0448\u0442\u0443\u0447\u043A\u0430! \u0410 \u043F\u043B\u0430\u0441\u0442\u0438\u043D\u043A\u0438
        \u0443 \u0432\u0430\u0441 \u0438\u043C\u0435\u044E\u0442\u0441\u044F?"
  - StateName: Chess
    lines:
    - text: "\"\u0411\u043E\u0433 \u043D\u0435 \u0438\u0433\u0440\u0430\u0435\u0442
        \u0432 \u043A\u043E\u0441\u0442\u0438\", \u0430 \u0432\u043E\u0442 \u0448\u0430\u0445\u043C\u0430\u0442\u044B
        - \u0434\u0440\u0443\u0433\u043E\u0435 \u0434\u0435\u043B\u043E!"
  - StateName: Vodka
    lines:
    - text: "\u0412\u044B \u0447\u0442\u043E, \u0430\u043B\u043A\u043E\u0433\u043E\u043B\u044C?
        \u0410 \u0432 \u043F\u043E\u0435\u0437\u0434\u0435 \u0440\u0430\u0437\u0432\u0435
        \u043C\u043E\u0436\u043D\u043E?"
  - StateName: Book
    lines:
    - text: "\u041D\u0438\u0447\u0435\u0433\u043E \u0441\u0435\u0431\u0435, \u0430
        \u043A\u0430\u043A \u044D\u0442\u043E \u0432 \u0433\u0430\u0431\u0430\u0440\u0438\u0442\u044B
        \u0440\u0443\u0447\u043D\u043E\u0439 \u043A\u043B\u0430\u0434\u0438 \u043F\u0440\u043E\u0448\u043B\u043E?"
  - StateName: Book
    lines:
    - text: "\u041D\u0438\u0447\u0435\u0433\u043E \u0441\u0435\u0431\u0435, \u0430
        \u043A\u0430\u043A \u044D\u0442\u043E \u0432 \u0433\u0430\u0431\u0430\u0440\u0438\u0442\u044B
        \u0440\u0443\u0447\u043D\u043E\u0439 \u043A\u043B\u0430\u0434\u0438 \u043F\u0440\u043E\u0448\u043B\u043E?"
  - StateName: Plane
    lines:
    - text: "\u041A\u0430\u043A\u043E\u0432\u0430 \u043B\u0438\u043D\u0438\u044F
        \u0444\u044E\u0437\u0435\u043B\u044F\u0436\u0430! \u041A\u0430\u043A \u0440\u0435\u0430\u043B\u0438\u0441\u0442\u0438\u0447\u043D\u043E,
        \u0434\u0430\u0436\u0435 \u043A\u0440\u044B\u043B\u043E \u043D\u0430 \u0441\u043A\u043E\u0442\u0447\u0435
        \u0434\u0435\u0440\u0436\u0438\u0442\u0441\u044F! \u041B\u0430\u0434\u043D\u043E,
        \u0440\u0430\u0437 \u043C\u044B \u0432 \u043F\u043E\u0435\u0437\u0434\u0435,
        \u043C\u043D\u0435 \u044D\u0442\u043E \u0432\u0440\u044F\u0434 \u043B\u0438
        \u043F\u0440\u0438\u0433\u043E\u0434\u0438\u0442\u0441\u044F"
