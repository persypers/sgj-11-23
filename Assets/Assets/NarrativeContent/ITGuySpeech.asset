  m_Script: {fileID: 11500000, guid: 0c1c47cbf3af15f4fa5be4f181cfd171, type: 3}
  m_Name: ITGuySpeech
  m_EditorClassIdentifier: 
  states:
  - StateName: Default
    lines:
    - text: "\u0423 \u0432\u0430\u0441 \u0435\u0441\u0442\u044C \u0433\u0434\u0435
        \u043D\u043E\u0443\u0442\u0431\u0443\u043A \u0437\u0430\u0440\u044F\u0434\u0438\u0442\u044C?
        \u0418 \u0441\u0430\u043C \u043D\u043E\u0443\u0442\u0431\u0443\u043A."
    - text: "\u0411\u0435\u0437 \u043A\u043E\u043C\u043F\u0430 \u0447\u0443\u0432\u0441\u0442\u0432\u0443\u044E
        \u043F\u043E\u0432\u044B\u0448\u0435\u043D\u043D\u0443\u044E \u0442\u0440\u0435\u0432\u043E\u0436\u043D\u043E\u0441\u0442\u044C."
    - text: "\u0421\u0432\u044F\u0437\u044C \u0437\u0434\u0435\u0441\u044C \u043F\u043B\u043E\u0445\u043E
        \u043B\u043E\u0432\u0438\u0442. \u041D\u0430\u0432\u0435\u0440\u043D\u043E\u0435.
        \u0423 \u043C\u0435\u043D\u044F \u043D\u0435\u0442 \u0434\u0435\u0432\u0430\u0439\u0441\u0430,
        \u0447\u0442\u043E\u0431\u044B \u043F\u0440\u043E\u0432\u0435\u0440\u0438\u0442\u044C."
    - text: "\u0425\u043C. \u0412\u043E\u043E\u0431\u0449\u0435 \u044F \u043F\u0440\u0435\u0434\u043F\u043E\u0447\u0438\u0442\u0430\u044E
        \u043B\u043E\u0443-\u0444\u0430\u0439 \u0444\u043E\u043D\u043A."
    - text: "\u0422\u0430\u043A, \u043F\u0430\u0441\u043F\u043E\u0440\u0442 - \u0447\u0435\u043A,
        \u0434\u043E\u0432\u0435\u0440\u0435\u043D\u043D\u043E\u0441\u0442\u044C
        - \u0447\u0435\u043A, \u0441\u043F\u0440\u0430\u0432\u043A\u0430 - \u0447\u0435\u043A.
        \u0412\u0440\u043E\u0434\u0435 \u0432\u0441\u0435 \u0432 \u043F\u043E\u0440\u044F\u0434\u043A\u0435."
  - StateName: Laptop
    lines:
    - text: "\u0420\u0435\u0431\u044F\u0442\u0430, \u044F \u043F\u0440\u043E\u043F\u0443\u0449\u0443
        \u0441\u0435\u0433\u043E\u0434\u043D\u044F \u043C\u0438\u0442\u0438\u043D\u0433,
        \u0443 \u043C\u0435\u043D\u044F \u044D\u043A\u0441\u0442\u0440\u0435\u043D\u043D\u044B\u0439
        \u0440\u0435\u043B\u043E\u043A\u0435\u0439\u0442."
    - text: "\u0410\u043B\u043B\u043E. \u0414\u0430. \u0410\u0433\u0430. \u0414\u0430,
        \u044F \u0432\u0437\u044F\u043B \u043F\u0435\u0440\u0432\u044B\u0435 \u0431\u0438\u043B\u0435\u0442\u044B
        \u043A\u043E\u0442\u043E\u0440\u044B\u0435 \u0431\u044B\u043B\u0438. \u041A\u0443\u0434\u0430?..
        \u042D-\u044D-\u044D. \u0429\u0430, \u043F\u043E\u0433\u043E\u0434\u0438.
        \u0410 \u043A\u0443\u0434\u0430 \u043F\u043E\u0435\u0437\u0434 \u0438\u0434\u0451\u0442?"
    - text: "\u042F \u043E\u0442\u043F\u0440\u0430\u0432\u0438\u043B \u043F\u0443\u043B\u043B-\u0440\u0435\u043A\u0432\u0435\u0441\u0442.
        \u0410? \u041D\u0435\u0442 \u043D\u0435 \u043F\u043E\u0444\u0438\u043A\u0441\u0438\u043B."
  - StateName: Easel
    lines:
    - text: "\u0410\u043D\u0430\u043B\u043E\u0433\u043E\u0432\u044B\u0439 \u0430\u0440\u0442?
        \u041D\u0443 \u0443\u0436 \u043D\u0435\u0442, \u0441\u043F\u0430\u0441\u0438\u0431\u043E."
  - StateName: Newspaper
    lines:
    - text: "\u0413\u0430\u0437\u0435\u0442\u0430? \u0422\u0430\u043C \u0436\u0435
        \u0441\u043F\u043B\u043E\u0448\u044C \u043F\u0440\u043E\u043F\u0430\u0433\u0430\u043D\u0434\u0430!"
  - StateName: Ball
    lines:
    - text: "\u041E, \u043C\u044B \u0441 \u0440\u0435\u0431\u044F\u0442\u0430\u043C\u0438
        \u0433\u043E\u043D\u044F\u043B\u0438 \u043C\u044F\u0447 \u043F\u043E\u0440\u043E\u0439...
        \u0434\u0430 \u0442\u043E\u043B\u044C\u043A\u043E \u0433\u0434\u0435 \u0442\u0435
        \u0440\u0435\u0431\u044F\u0442\u0430 \u0442\u0435\u043F\u0435\u0440\u044C?"
  - StateName: Ball
    lines:
    - text: "\u041E\u0439 \u043D\u0435 \u043D\u0430\u043F\u043E\u043C\u0438\u043D\u0430\u0439\u0442\u0435,
        \u0432\u044B \u0446\u0435\u043D\u044B \u0442\u043E \u043D\u0430 \u0431\u0438\u043B\u0435\u0442\u044B
        \u0432\u0438\u0434\u0435\u043B\u0438?"
