  states:
  - StateName: Default
    lines:
    - text: "\u0411\u0430\u0431\u0443\u0448\u043A\u0430 \u043D\u0435 \u0440\u0430\u0437\u0440\u0435\u0448\u0430\u0435\u0442
        \u043C\u043D\u0435 \u043A\u043E\u0432\u044B\u0440\u044F\u0442\u044C\u0441\u044F
        \u0432 \u043D\u043E\u0441\u0443. \u041D\u043E \u044F \u0432\u0441\u0451 \u0440\u0430\u0432\u043D\u043E
        \u043A\u043E\u0432\u044B\u0440\u044F\u044E\u0441\u044C."
    - text: "\u041C\u043D\u0435 \u043D\u0435 \u0432\u0435\u0441\u0435\u043B\u043E.
        \u041A\u043E\u0433\u0434\u0430 \u0431\u0443\u0434\u0435\u0442 \u0432\u0435\u0441\u0435\u043B\u043E?"
    - text: "\u041C\u044B \u0443\u0436\u0435 \u043F\u0440\u0438\u0435\u0445\u0430\u043B\u0438?"
    - text: "\u041C\u044B \u043F\u0440\u0438\u0435\u0445\u0430\u043B\u0438?"
    - text: "\u0410 \u043A\u043E\u0433\u0434\u0430 \u043C\u044B \u043F\u0440\u0438\u0435\u0434\u0435\u043C?"
    - text: "\u0410 \u043A\u0443\u0434\u0430 \u043C\u044B \u0435\u0434\u0435\u043C?"
    - text: "\u0413\u0434\u0435 \u043C\u044B \u0435\u0434\u0435\u043C?"
    - text: "\u0410 \u0441\u0435\u0439\u0447\u0430\u0441 \u0433\u0434\u0435 \u043C\u044B
        \u0435\u0434\u0435\u043C?"
    - text: "\u0410 \u043C\u044B \u043F\u0440\u0438\u0435\u0434\u0435\u043C?"
    - text: "\u041C\u044B \u0435\u0434\u0435\u043C?"
    - text: "\u0410\u2026 \u0430\u2026 \u0430 \u043A\u043E\u0433\u0434\u0430 \u043C\u044B
        \u043F\u043E\u0435\u0434\u0435\u043C?"
    - text: "\u041A\u043E\u0433\u0434\u0430 \u044F \u0432\u044B\u0440\u0430\u0441\u0442\u0443,
        \u044F \u0445\u043E\u0447\u0443 \u0431\u044B\u0442\u044C \u043A\u0430\u043A
        \u0432\u044B! \u0422\u043E\u043B\u044C\u043A\u043E \u043B\u0443\u0447\u0448\u0435!"
  - StateName: Ball
    lines:
    - text: "\u0425\u0430\u0445\u0430\u0445\u0430\u0445\u0430\u0445\u0430\u0445\u0430\u0445\u0430"
    - text: "\u041B\u043E\u0432\u0438!"
    - text: "\u041D\u0435 \u043F\u043E\u0439\u043C\u0430\u0435\u0448\u044C!"
  - StateName: Magazine
    lines:
    - text: "\u042D\u0442\u043E \u043C\u0430\u043C\u0430?"
    - text: "\u042D\u0442\u043E \u043F\u0430\u043F\u0430?"
    - text: "\u0427\u0442\u043E \u043E\u043D\u0438 \u0434\u0435\u043B\u0430\u044E\u0442?"
    - text: "\u0411\u0430\u0431\u0443\u0448\u043A\u0430, \u0430 \u0442\u044B \u0442\u0430\u043A
        \u043C\u043E\u0436\u0435\u0448\u044C?"
  messageShowCooldown: 3
