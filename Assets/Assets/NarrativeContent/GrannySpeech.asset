  m_Script: {fileID: 11500000, guid: 0c1c47cbf3af15f4fa5be4f181cfd171, type: 3}
  m_Name: GrannySpeech
  m_EditorClassIdentifier: 
  states:
  - StateName: Default
    lines:
    - text: "\u0412\u044B \u0432\u0438\u0434\u0435\u043B\u0438 \u0447\u0442\u043E
        \u041A\u043E\u043B\u044F \u0441 \u0424\u0438\u043B\u0438\u043F\u043F\u043E\u043C
        \u0443\u0447\u0443\u0434\u0438\u043B\u0438 \u043F\u043E \u0442\u0435\u043B\u0435\u0432\u0438\u0437\u043E\u0440\u0443?
        \u0421\u0434\u0443\u0440\u0435\u043B\u0438 \u0441\u043E\u0432\u0441\u0435\u043C!"
    - text: "\u0412\u0435\u0442\u0440\u0435\u043D\u043E \u0441\u0435\u0433\u043E\u0434\u043D\u044F
        \u0431\u0443\u0434\u0435\u0442. \u042F \u0432\u0438\u0434\u0435\u043B\u0430
        \u0432\u0447\u0435\u0440\u0430 \u0437\u0430\u043A\u0430\u0442 \u043A\u0440\u0430\u0441\u043D\u044E\u0449\u0438\u0439
        \u0431\u044B\u043B."
    - text: "\u041D\u0443, \u044F, \u043A\u043E\u043D\u0435\u0447\u043D\u043E, \u043B\u044F\u0433\u0443\u0448\u043A\u0430
        \u043F\u0443\u0442\u0435\u0448\u0435\u0441\u0442\u0432\u0435\u043D\u043D\u0438\u0446\u0430.
        \u041E\u0445, \u0432\u0441\u0451. \u0415\u0434\u0435\u043C."
    - text: "\u0410 \u044F\u0438\u0447\u043A\u0438 \u0442\u043E \u044F \u0437\u0430\u0431\u044B\u043B\u0430!
        \u0412\u043E\u0442 \u0440\u0430\u0437\u0437\u044F\u0432\u0430. \u041D\u0430\u0432\u0430\u0440\u0438\u043B\u0430
        \u0438 \u043E\u0441\u0442\u0430\u0432\u0438\u043B\u0430 \u0434\u043E\u043C\u0430.
        \u0412 \u0445\u043E\u043B\u043E\u0434\u0438\u043B\u044C\u043D\u0438\u043A\u0435
        \u043D\u0430\u0432\u0435\u0440\u043D\u043E\u0435 \u043E\u0441\u0442\u0430\u0432\u0438\u043B\u0430.
        \u0418\u043B\u0438 \u043D\u0430 \u043F\u043B\u0438\u0442\u0435."
    - text: "\u041D\u0435\u0442, \u043D\u0430 \u043F\u043B\u0438\u0442\u0435 \u043D\u0430\u0432\u0435\u0440\u043D\u043E\u0435
        \u043E\u0441\u0442\u0430\u0432\u0438\u043B\u0430. \u041E\u0445, \u0434\u044B\u0440\u044F\u0432\u0430\u044F
        \u0433\u043E\u043B\u043E\u0432\u0430 \u043C\u043E\u044F. \u0412\u043E\u0442
        \u043D\u0430\u0432\u0430\u0440\u0438\u043B\u0430 \u044F\u0438\u0447\u0435\u043A
        \u0438 \u043E\u0441\u0442\u0430\u0432\u0438\u043B\u0430. \u041E\u0439, \u043A\u0430\u043A
        \u0432\u0441\u0435\u0433\u0434\u0430."
  - StateName: Magazine
    lines:
    - text: "\u0421\u0440\u0430\u043C \u0442\u043E \u043A\u0430\u043A\u043E\u0439!"
    - text: "\u041C\u0435\u0440\u0437\u043E\u0441\u0442\u044C!"
    - text: "\u041A\u0430\u043A\u0430\u044F \u043F\u043E\u0448\u043B\u043E\u0441\u0442\u044C!"
    - text: "\u0423\u0436\u0430\u0441, \u0447\u0442\u043E \u0442\u0432\u043E\u0440\u0438\u0442\u0441\u044F!"
    - text: "\u0412\u043E\u0442 \u043C\u043E\u043B\u043E\u0434\u0451\u0436\u044C
        \u043F\u043E\u0448\u043B\u0430! \u0411\u0435\u0441\u0441\u0442\u044B\u0436\u0438\u0435!"
  messageShowCooldown: 5
