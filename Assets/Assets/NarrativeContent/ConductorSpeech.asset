  m_Script: {fileID: 11500000, guid: 0c1c47cbf3af15f4fa5be4f181cfd171, type: 3}
  m_Name: ConductorSpeech
  m_EditorClassIdentifier: 
  states:
  - StateName: Default
    lines:
    - text: "\u041D\u0435\u0432\u0435\u0440\u043E\u044F\u0442\u043D\u043E\u0435 \u0441\u0442\u0435\u0447\u0435\u043D\u0438\u0435
        \u043E\u0431\u0441\u0442\u043E\u044F\u0442\u0435\u043B\u044C\u0441\u0442\u0432!
        \u041E\u0440\u043A\u0435\u0441\u0442\u0440 \u0443\u0435\u0445\u0430\u043B
        \u0431\u0435\u0437 \u043C\u0435\u043D\u044F!"
    - text: "\u0421\u0442\u0443\u043A \u043A\u043E\u043B\u0451\u0441 \u0442\u0430\u043A
        \u0443\u0441\u043F\u043E\u043A\u0430\u0438\u0432\u0430\u0435\u0442. \u041D\u043E
        \u043D\u0435 \u043C\u0435\u043D\u044F."
    - text: "\u0427\u0442\u043E \u0432\u044B \u0441\u043C\u043E\u0442\u0440\u0438\u0442\u0435
        \u0442\u0430\u043A \u043D\u0430 \u043C\u0435\u043D\u044F. \u0427\u0442\u043E-\u0442\u043E
        \u0441\u043B\u0443\u0447\u0438\u043B\u043E\u0441\u044C?"
    - text: "\u0421 \u043D\u0435\u043A\u043E\u0442\u043E\u0440\u043E\u0433\u043E
        \u0432\u0440\u0435\u043C\u0435\u043D\u0438 \u0438\u0441\u043F\u044B\u0442\u044B\u0432\u0430\u044E
        \u043A\u0430\u043A\u043E\u0435-\u0442\u043E \u0431\u0435\u0441\u043F\u043E\u043A\u043E\u0439\u0441\u0442\u0432\u043E
        \u043F\u0440\u0438 \u0432\u0438\u0434\u0435 \u043F\u043E\u0435\u0437\u0434\u0430..."
  - StateName: Guitar
    lines:
    - text: "\u0413\u0434\u0435 \u0442\u0440\u0438 \u0430\u043A\u043A\u043E\u0440\u0434\u0430
        \u0434\u0443\u0448\u0443 \u0440\u0432\u0443\u0442, \u0442\u0430\u043C \u043C\u0443\u0437\u044B
        \u043F\u044C\u044F\u043D\u044B\u0435 \u043F\u043E\u044E\u0442. \u041E \u0431\u043E\u0436\u0435,
        \u043D\u0430 \u0433\u043B\u0430\u0433\u043E\u043B\u044B..."
  - StateName: VinylPlayer
    lines:
    - text: "\u0412 \u043F\u043B\u0435\u043D\u0443 \u0443 \u0432\u0438\u043D\u0438\u043B\u0430
        \u0432\u0438\u0431\u0440\u0430\u0446\u0438\u044F \u0441\u0442\u0440\u0443\u043D,
        \u0435\u0435 \u0432\u044B\u043F\u0443\u0441\u043A\u0430\u0435\u0442 \u0441
        \u0438\u0433\u043B\u043E\u044E \u0441\u043A\u0430\u043A\u0443\u043D. "
  - StateName: Popit
    lines:
    - text: "\u0423 \u0432\u0430\u0441 \u043F\u043E\u043F-\u0438\u0442, \u043E\u0431\u0438\u043B\u044C\u043D\u044B\u0439
        \u0438 \u043A\u0440\u0430\u0441\u0438\u0432\u044B\u0439? \u0413\u0434\u0435
        \u043C\u043B\u0430\u0434\u0448\u0438\u0439 \u0431\u0440\u0430\u0442 \u0435\u0433\u043E,
        \u0447\u0442\u043E \u0441\u043B\u0430\u0432\u0435\u043D \u043C\u0438\u043B\u043E\u0442\u043E\u0439?"
  - StateName: Pumpkin
    lines:
    - text: "\u0421\u0435\u0439 \u043A\u043E\u0440\u043D\u0435\u043F\u043B\u043E\u0434
        \u2013 \u043A \u041A\u043E\u0448\u043C\u0430\u0440\u0443 \u043F\u0435\u0440\u0435\u0434
        \u0440\u043E\u0436\u0434\u0435\u0441\u0442\u0432\u043E\u043C \u043E\u0442\u0441\u044B\u043B\u043A\u0430? "
  - StateName: Pillow
    lines:
    - text: "\u0412\u044B \u0432\u0435\u0440\u043D\u043E \u0443\u043A\u043E\u043B\u043E\u0442\u044C
        \u043C\u0435\u043D\u044F \u0445\u043E\u0442\u0438\u0442\u0435? \u041E\u0440\u043A\u0435\u0441\u0442\u0440\u0430
        \u043D\u0435\u0442, \u0430 \u0434\u0438\u0440\u0438\u0436\u0451\u0440 \u043E\u0441\u0442\u0430\u043B\u0441\u044F,
        \u043D\u043E \u044D\u0442\u043E, \u043F\u0440\u0430\u0432\u043E, \u0432\u043E\u0432\u0441\u0435
        \u043D\u0435 \u0441\u043C\u0435\u0448\u043D\u043E!"
