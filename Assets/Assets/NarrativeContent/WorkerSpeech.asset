  m_Script: {fileID: 11500000, guid: 0c1c47cbf3af15f4fa5be4f181cfd171, type: 3}
  m_Name: WorkerSpeech
  m_EditorClassIdentifier: 
  states:
  - StateName: Default
    lines:
    - text: "\u041D\u0438\u043A\u0430\u043A \u043D\u0435 \u043C\u043E\u0433\u0443
        \u043F\u043E\u043D\u044F\u0442\u044C \u0433\u0434\u0435 \u0433\u0430\u043B\u044C\u044E\u043D."
    - text: "\u0422\u043E\u0442 \u0433\u043E\u0434 \u0435\u0437\u0434\u0438\u043B
        \u0432 \u0441\u0430\u043D\u0430\u0442\u043E\u0440\u0438\u0439 \u043F\u043E
        \u043F\u0443\u0442\u0451\u0432\u043A\u0435 - \u0442\u0430\u043A \u0442\u0430\u043C
        \u0440\u0430\u043A\u043E\u0432 \u0434\u0430\u0432\u0430\u043B\u0438."
    - text: "\u0412 \u0448\u0430\u0445\u043C\u0430\u0442\u044B \u0441\u044B\u0433\u0440\u0430\u0442\u044C
        \u0431\u044B\u043B\u043E \u0431\u044B \u0434\u043E\u0431\u0440\u043E."
    - text: "\u041D\u0430\u0433\u0430\u0434\u0438\u043B\u0438 \u043E\u043D\u0438
        \u043D\u0430\u043C \u043A\u043E\u043D\u0435\u0447\u043D\u043E, \u043D\u043E
        \u043C\u044B \u0441\u0432\u043E\u0435 \u0432\u043E\u0437\u044C\u043C\u0451\u043C!"
    - text: "\u0421\u043A\u0443\u0447\u043D\u043E \u0442\u0443\u0442. \u0421\u044B\u0433\u0440\u0430\u0442\u044C
        \u0431\u044B \u0441 \u043A\u0435\u043C \u0432 \u043A\u0430\u0440\u0442\u044B,
        \u0432 \u0448\u0430\u0445\u043C\u0430\u0442\u044B - \u0434\u0430\u043A \u0443
        \u0432\u0441\u0435\u0445 \u0441\u043E\u0442\u0438\u043A\u0438 \u0432 \u0440\u0443\u043A\u0430\u0445."
  - StateName: Magazine
    lines:
    - text: "\u042D\u0442\u043E \u043A\u0442\u043E, \u041F\u043E\u043B\u0438\u043D\u043A\u0430
        \u0447\u0442\u043E \u043B\u0438? \u041F\u043E\u0445\u043E\u0440\u043E\u0448\u0435\u043B\u0430,
        \u0440\u0430\u0441\u0446\u0432\u0435\u043B\u0430!"
  - StateName: Laptop
    lines:
    - text: "\u0412\u043E \u0434\u0430\u0451\u0442 \u043F\u0440\u043E\u0433\u0440\u0435\u0441\u0441!
        \u042F \u0431 \u0442\u0430\u043A\u0438\u0435 \u0441\u0430\u043C \u0441\u043E\u0431\u0438\u0440\u0430\u043B,
        \u0434\u0430 \u043F\u0430\u043B\u044C\u0446\u044B \u0443\u0436\u0435 \u043D\u0435
        \u0442\u0435."
  - StateName: Uniform
    lines:
    - text: "\u0412\u043E\u0442 \u0432\u0435\u0434\u044C \u0448\u0432\u0435\u0439\u043D\u044B\u0435
        \u0432\u043E\u0439\u0441\u043A\u0430! \u041C\u044B \u0441\u0435\u0431\u0435
        \u0442\u0430\u043A\u043E\u0433\u043E \u043D\u0435 \u043F\u043E\u0437\u0432\u043E\u043B\u044F\u043B\u0438"
  - StateName: Vodka
    lines:
    - text: "\u0422\u044B \u043C\u0435\u043D\u044F \u043D\u0435 \u043F\u0440\u043E\u0432\u043E\u0446\u0438\u0440\u0443\u0439.
        \u0413\u043E\u0434 \u0432 \u0437\u0430\u0432\u044F\u0437\u043A\u0435 \u0443\u0436\u0435."
  - StateName: Pies
    lines:
    - text: "\u0423 \u043D\u0430\u0441 \u0432 \u0444\u0430\u0437\u0430\u043D\u043A\u0435
        \u0432 \u0441\u0442\u043E\u043B\u043E\u0432\u043E\u0439 \u0442\u0430\u043A\u0438\u0435
        \u0431\u044B\u043B\u0438. \u041A\u0430\u043A \u0443\u0437\u043D\u0430\u043B
        \u0441 \u0447\u0435\u043C \u043E\u043D\u0438 - \u0442\u0430\u043A \u0438
        \u043D\u0435 \u0435\u043C \u0431\u043E\u043B\u044C\u0448\u0435."
  - StateName: Vinyl
    lines:
    - text: "\u042F \u0442\u0430\u043A\u043E\u0435 \u043D\u0435 \u0441\u043B\u0443\u0448\u0430\u044E."
